{
  "compilerOptions": {
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "nodenext",
    "target": "es2015",
    "lib": [
      "dom",
      "es2021"
    ],
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "jsx": "react",
  }
}
